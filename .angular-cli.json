{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "FCDR"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "bootstarp",
        "GCWeb",
        "fonts",
        "css",
        "distF",
        "039978303943.jpeg",
        "wet-boew"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/css/jquery.dataTables.min.css",
        "assets/css/colReorder.dataTables.min.css",
        "assets/css/buttons.dataTables.min.css"
      ],
      "scripts": [
        "assets/scripts/keycloak.js",
        "assets/js/jquery.min.js",
        "assets/js/jquery.dataTables.min.js",
        "assets/js/dataTables.colReorder.min.js",
        "assets/js/dataTables.buttons.min.js",
        "assets/js/buttons.colVis.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts",
        "local": "environments/environment.local.ts",
        "hre-dev": "environments/environment.hre-dev.ts",
        "azure-dev": "environments/environment.azure-dev.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
